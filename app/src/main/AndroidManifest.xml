    package="pdasolucoes.com.br.homevacation">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OpcaoEntradaActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".CadastroAmbienteActivity"></activity>

        <activity android:name=".CadastroItemActivity"></activity>

        <activity android:name=".CadastroQuestaoActivity"></activity>

        <activity android:name=".CheckListAmbienteActivity"></activity>

    </application>

</manifest>